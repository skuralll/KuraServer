{
  "format_version": "1.10.0",
  "particle_effect": {
    "description": {
      "identifier": "kuraserver:assassin_ultimate",
      "basic_render_parameters": {
        "material": "particles_alpha",
        "texture": "textures/particle/minerult"
      }
    },
    "components": {
     "minecraft:emitter_local_space": {
        "position": true,
        "rotation": true
      },

      "minecraft:emitter_rate_instant": {
        "num_particles": 1
      },

      "minecraft:emitter_lifetime_once": {},

      "minecraft:emitter_shape_custom": {
        "offset": [ 0, 0, 0],
        "direction": [ 1.0, 0.0, 0.0 ]
      },

      "minecraft:particle_initial_spin": {
        "rotation": "Math.random(0, 360)",
        "rotation_rate": 20
      },

      "minecraft:particle_motion_parametric": {
      	"rotation": "variable.particle_age * 360",
      	"relative_position": [0, "-(variable.particle_age * 1.5) * (variable.particle_age * 1.5) * 1.2", 0]
	  },

      "minecraft:particle_lifetime_expression": {
        "max_lifetime": 1.3
      },

      "minecraft:particle_motion_collision": {
        "enabled": 0
      },

      "minecraft:particle_appearance_billboard": {
        "size": [ "(2 - (1 / (variable.particle_age * 8))) * 1.2", "(2 - (1 / (variable.particle_age * 8))) * 1.2"],
        "facing_camera_mode": "direction_y",

        "uv": {
          "texture_width": 256,
          "texture_height": 256,
          "uv": [ 0, 0],
          "uv_size": [ 256, 256]
        }
      },

      "minecraft:particle_appearance_tinting": {
        "color": [ "(1.3 - variable.particle_age) / 2.6", "(1.3 - variable.particle_age) / 2.6", "(1.3 - variable.particle_age) / 2.6"]
      }
    }
  }
}
